<p id="notice"><%= notice %></p>

<h1>Listing Activitytypes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activitytypes.each do |activitytype| %>
      <tr>
        <td><%= activitytype.description %></td>
        <td><%= link_to 'Show', activitytype %></td>
        <td><%= link_to 'Edit', edit_activitytype_path(activitytype) %></td>
        <td><%= link_to 'Destroy', activitytype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activitytype', new_activitytype_path %>
